{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"Moicrosoft.Hosting.Lifetime": "Information"
			}
		},
		"WriteTo": [
			{
				"Name": "File",
				"Args": {
					"path": "./logs/log-.txt",
					"rollingInterval": "Day"
				}
			}			
		]
	},
	"AllowedHosts": "*"
}